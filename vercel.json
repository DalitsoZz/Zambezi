{
  "version": 2,
  "builds": [
    {
      "src": "src/app.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/src/app.js"
    },
    {
      "src": "/tourism",
      "dest": "/public/tourism.html"
    },
    {
      "src": "/news-detail",
      "dest": "/public/news-detail.html"
    },
    {
      "src": "/(.*)",
      "dest": "/src/app.js"
    }
  ],
  "functions": {
    "src/app.js": {
      "includeFiles": "public/**"
    }
  }
}
